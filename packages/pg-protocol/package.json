{
  "name": "pg-protocol",
  "version": "1.6.1",
  "description": "The postgres client/server binary protocol, implemented in TypeScript",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./messages": {
      "import": "./dist/esm/messages.js",
      "require": "./dist/messages.js"
    },
    "./parser": {
      "import": "./dist/esm/parser.js",
      "require": "./dist/parser.js"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^10.0.7",
    "@types/node": "^12.12.21",
    "chai": "^4.2.0",
    "chunky": "^0.0.0",
    "mocha": "^10.5.2",
    "ts-node": "^8.5.4",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "test": "mocha dist/*.test.js",
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:watch": "tsc --watch",
    "prepublish": "yarn build",
    "pretest": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/brianc/node-postgres.git",
    "directory": "packages/pg-protocol"
  },
  "files": [
    "/dist/*{js,ts,map}",
    "/dist/esm/*{js,ts,map}",
    "/src"
  ]
}
