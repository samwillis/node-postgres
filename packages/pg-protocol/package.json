{
  "name": "pg-protocol",
  "version": "1.6.1",
  "description": "The postgres client/server binary protocol, implemented in TypeScript",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^10.0.7",
    "@types/node": "^12.12.21",
    "chai": "^4.2.0",
    "mocha": "^10.5.2",
    "ts-node": "^8.5.4",
    "tsup": "^8.2.4",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "test": "mocha dist/**/*.test.{js,mjs}",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "prepublish": "yarn build",
    "pretest": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/brianc/node-postgres.git",
    "directory": "packages/pg-protocol"
  },
  "files": [
    "/dist/*{js,ts,mjs,mts,map}",
    "/src"
  ]
}
